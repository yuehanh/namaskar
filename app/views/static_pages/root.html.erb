<script type="text/javascript">
  Object.assign(window.images,{
    splashPhoto: "<%= asset_path("splashphoto.jpg") %>",
    logo: "<%= asset_path("logo.svg") %>",
  })
</script>
<% if logged_in? %>
  <script id="bootstrap" type="text/javascript">
    window.currentState = <%= render("api/users/current_user.json.jbuilder", user: current_user).html_safe %>
